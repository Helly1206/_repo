<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.streamgenerator"
       name="Kodi Stream Generator"
       version="0.9.1"
       provider-name="Helly">
  <requires>
    <import addon="xbmc.python" version="2.26.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py" />
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Kodi Stream Generator</summary>
    <summary lang="nl">Kodi Stream Generator</summary>
    <description lang="en">This addon generates .strm files from favorites, urls or files to be integrated in the library</description>
    <description lang="nl">Deze addon genereert .strm bestanden van favorieten, url's of bestanden om in de bibiliotheek geintegreerd te worden</description>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website></website>
    <email></email>
    <source>https://github.com/Helly1206/script.streamgenerator</source>
  </extension>
</addon>
